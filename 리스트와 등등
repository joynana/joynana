score = [95.5,88.0,82.6,80.0]
score[1:3] = [90.0,80.5]
#오답: print('최종점수:%d'.format(score))
print('최종점수:{}'.format(score))

#평균 구하기
print(sum(score)/4)
print('평균점수:{}'.format(sum(score)/4))

